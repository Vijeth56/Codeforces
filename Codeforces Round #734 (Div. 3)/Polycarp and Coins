#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int j = n/3;
        int k = n/3;
        if(j + (2*j) == n)
            cout<<j<<" "<<k<<"\n";
        else if(j + (2*j) != n && j + (2*(j+1)) == n)
            cout<<j<<" "<<k+1<<"\n";
        else
            cout<<j+1<<" "<<k<<"\n";
        
    }
 
    return 0;
}
